** start of script.js **

function largestOfAll(arr) {
  let result = [];

  for (let i = 0; i < arr.length; i++) {
    // Start with the first element of the sub-array
    let largest = arr[i][0];

    // Loop through each element of the sub-array
    for (let j = 1; j < arr[i].length; j++) {
      if (arr[i][j] > largest) {
        largest = arr[i][j];
      }
    }

    // Push the largest number of this sub-array into result
    result.push(largest);
  }

  return result;
}


** end of script.js **

